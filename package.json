  "description": "FS金彩赛车游戏网站",
  "type": "module",
  "main": "backend.js",
  "scripts": {
    "start": "node backend.js",
    "start:agent": "node agentBackend.js",
    "start:all": "concurrently \"npm start\" \"npm run start:agent\"",
    "build": "echo '确认前端文件存在...' && node -e \"if(require('fs').existsSync('./deploy/frontend')) console.log('前端目录已确认'); else console.error('找不到前端目录!');\"",
    "dev": "nodemon backend.js",
    "dev:agent": "nodemon agentBackend.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:agent\"",
    "check-deploy": "node check-deploy.js",
    "fix-db": "node fix-db-issues.js",
    "test-db": "node test-db-queries.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check:env": "node deploy/check-env.js"
  },
  "keywords": [],
